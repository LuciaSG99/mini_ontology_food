#################################################################
#    Object Properties
#################################################################

###  http://example.org/food#hasIngredient
:hasIngredient rdf:type owl:ObjectProperty ;
               rdfs:domain :Food ;
               rdfs:range :Ingredient ;
               rdfs:comment "Relates a food to one of its ingredients."@en ;
               rdfs:label "has ingredient"@en .


###  http://example.org/food#suitableForDiet
:suitableForDiet rdf:type owl:ObjectProperty ;
                 rdfs:domain :Food ;
                 rdfs:range :Diet ;
                 rdfs:comment "Indicates that a food is appropriate for a given diet."@en ;
                 rdfs:label "suitable for diet"@en .


#################################################################
#    Data properties
#################################################################

###  http://example.org/food#caloriesPer100g
:caloriesPer100g rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Food ;
                 rdfs:range xsd:integer ;
                 rdfs:comment "Energy value per 100 grams of food."@en ;
                 rdfs:label "calories per 100g"@en .


#################################################################
#    Classes
#################################################################

###  http://example.org/food#Diet
:Diet rdf:type owl:Class ;
      rdfs:comment "A set of dietary rules or eating patterns."@en ;
      rdfs:label "Diet"@en .


###  http://example.org/food#Food
:Food rdf:type owl:Class ;
      rdfs:comment "Any edible item intended for human consumption."@en ;
      rdfs:label "Food"@en .


###  http://example.org/food#Ingredient
:Ingredient rdf:type owl:Class ;
            rdfs:comment "A component used to prepare a food."@en ;
            rdfs:label "Ingredient"@en .


###  http://example.org/food#VeganFood
:VeganFood rdf:type owl:Class ;
           rdfs:subClassOf :Food ;
           rdfs:comment "Food that is suitable for a vegan diet."@en ;
           rdfs:label "Vegan food"@en .


###  http://xmlns.com/foaf/0.1/Person
<http://xmlns.com/foaf/0.1/Person> rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  http://example.org/food#OliveOil
:OliveOil rdf:type owl:NamedIndividual ,
                   :Ingredient ;
          rdfs:label "Olive oil"@en .


###  http://example.org/food#Pasta
:Pasta rdf:type owl:NamedIndividual ,
                :Ingredient ;
       rdfs:label "Pasta"@en .


###  http://example.org/food#PastaSalad
:PastaSalad rdf:type owl:NamedIndividual ,
                     :Food ;
            :hasIngredient :OliveOil ,
                           :Pasta ,
                           :Tomato ;
            :suitableForDiet :VeganDiet ;
            :caloriesPer100g 150 ;
            rdfs:comment "A simple pasta salad made with tomato and olive oil."@en ;
            rdfs:label "Pasta salad"@en .


###  http://example.org/food#Tomato
:Tomato rdf:type owl:NamedIndividual ,
                 :Ingredient ;
        rdfs:label "Tomato"@en .


###  http://example.org/food#VeganDiet
:VeganDiet rdf:type owl:NamedIndividual ,
                    :Diet ;
           rdfs:label "Vegan diet"@en .


